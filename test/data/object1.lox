class Bagel {
    eat() {
        print "yum!";
    }
}

var bagel = Bagel();
bagel.toasted = true;
bagel.kind = "sesame";

print bagel.toasted;  // should print "true"
print bagel.kind;  // should print "sesame"
bagel.eat();  // should print "yum!"

bagel.kind = "everything";
print bagel.kind;  // should print "everything"